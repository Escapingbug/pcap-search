export PS1="`whoami`@`hostname`:\w Dshell> "
dir=$(dirname "$0")
export BINPATH=$dir/bin
export DSHELL=$dir
export DATAPATH=$dir/share
export DECODERPATH=$dir/decoders
export LIBPATH=$dir/lib
export PATH=$BINPATH:$PATH
export LD_LIBRARY_PATH=$LIBPATH:$LD_LIBRARY_PATH
export PYTHONPATH=$DSHELL:$LIBPATH:$LIBPATH/output:$LIBPATH/python2.7/site-packages:$PYTHONPATH
